<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dani.trainingandroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".BasicFirstAppActivity"
            android:label="@string/title_basic_first_app"/>
        <activity
            android:name=".DisplayMessageActivity"
            android:label="@string/title_basic_first_app" />
        <activity android:name=".BasicFragmentActivity"
            android:label="@string/title_basic_fragment">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>